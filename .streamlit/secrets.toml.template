# Streamlit Secrets Configuration
# This file is used for Streamlit Cloud deployment
# Add your secrets in the Streamlit Cloud dashboard under Settings > Secrets

[github]
token = "your_github_personal_access_token"
owner = "your_github_username"
repo_name = "diabetes-risk-models"

[app]
name = "Diabetes Risk Analysis Platform"
version = "1.0.0"
debug = false

[deployment]
auto_upload_models = true
create_releases = false